{
  "dashboard": {
    "id": null,
    "uid": "cg-minimal",
    "title": "ClusterGenie Minimal",
    "timezone": "browser",
    "schemaVersion": 30,
    "version": 1,
    "panels": [
      {
        "type": "stat",
        "title": "Worker Queue Length",
        "datasource": "Prometheus",
        "targets": [{ "expr": "clustergenie_workerpool_queue_length", "refId": "A" }],
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 6 }
      },
      {
        "type": "stat",
        "title": "Rate-limit rejections/min",
        "datasource": "Prometheus",
        "targets": [{ "expr": "sum(rate(clustergenie_rate_limit_exceeded_total[1m]))", "refId": "A" }],
        "gridPos": { "x": 12, "y": 0, "w": 12, "h": 6 }
      },
      {
        "type": "timeseries",
        "title": "Job processing p95",
        "datasource": "Prometheus",
        "targets": [{ "expr": "histogram_quantile(0.95, sum(rate(clustergenie_job_processing_seconds_bucket[5m])) by (le))", "refId": "A" }],
        "gridPos": { "x": 0, "y": 6, "w": 24, "h": 8 }
      }
    ]
  },
  "overwrite": true
}
